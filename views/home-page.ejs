<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <title>Chinar Academy</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="https://kit.fontawesome.com/4ed4d66ea3.js" crossorigin="anonymous"></script>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600&family=Ubuntu&display=swap"
        rel="stylesheet">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <!-- Own CSS -->
    <link rel="stylesheet" href="/css/style-home.css" type="text/css">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>


   
</head>

<body style="position: relative;">


    <header>
        <div class="container-fluid px-0">
            <div class="main-bar">
                <div class="row">
                    <div class="col-lg-3"><i class="fab fa-canadian-maple-leaf fa-2x "></i><span
                            class="web-title">ChinarAcademy</span><i class="fas fa-university fa-2x"></i></a>
                    </div>
                    <div class="col-lg-6">
                        <form action="/search" method="GET">
                            <div class="input-group rounded search-box">
                                <input type="search" class="form-control rounded" placeholder="Search" name="q" />
                                <span class="input-group-text border-0" id="search-addon">
                                    <button class="btn btn-light btn-outline-dark">Search</button>
                                </span>
                        </form>
                    </div>
                </div>
                <div class="col-lg-3 main-profile">
                    <div class="dropdown">
                        <button class="btn dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown"
                            aria-haspopup="true" aria-expanded="false">
                            <img class="drop-img" src="Images/profile-icon.jpg">
                        </button>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton" style="text-align: center;">
                            <a href="/logout" class="dropdown-item " style="border: 1px solid black;
                                border-radius: 10px;background-color: silver;"> Logout </a>
                            <a href="/profile" class="dropdown-item " style="border: 1px solid black;
                                border-radius: 10px;border-top: none;background-color: silver;">Profile</a>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </header>
    <main>
        <div class="container-fluid main-one">
            <div class="row">
                <div class="col-lg-2 px-0 main-left">
                    <ul class="ul-one">
                        <li><i class="fas fa-home side-bar-icon fa-2x"></i><a class="side-links" href=""><span
                                    class="link-text">Home</span></a></li>
                        <li><i class="fab fa-hotjar side-bar-icon fa-2x"></i><a class="side-links" href="/courses"><span
                                    class="link-text">All Courses</span></a></li>
                        <li><i class="fas fa-compass side-bar-icon fa-2x"></i><a class="side-links"
                                href="/courses/browse"><span class="link-text">Browse Courses</span></a></li>

                    </ul>
                    <hr class="horizontal-rule">

                    <ul class="ul-two">
                        <li><i class="fas fa-history side-bar-icon fa-2x"></i><a class="side-links"
                                href="/playlist"><span class="link-text">Playlist</span></a></li>

                        <li><i class="fas fa-clock side-bar-icon fa-2x"></i><a class="side-links"
                                href="/resources"><span class="link-text">Academic Material</span></a></li>
                        <li><i class="fas fa-hands-helping side-bar-icon fa-2x"></i><a class="side-links" href="/help"><span
                                    class="link-text">Help</span></a></li>
                    </ul>
                    <hr style="margin-bottom: 0;" class="horizontal-rule">

                    <div class="history">
                        <div class="history-heading">History</div>
                        <div class="btn-group-vertical btn-group" role="group" aria-label="Basic example">

                            <% if(beta!=null){ %>
                            <% if(beta.course_names.length>10) {%>
                            <% for(let x=0;x< 10;x++) {%>
                            <a type="button" class="btn history-button btn-outline-dark"
                                href="/courses/<%= beta.course_array[x] %> "><%= beta.course_names[x] %> </a>
                            <% } %>
                            <% } else{ %>
                            <% for(let x=0;x< beta.course_array.length;x++) {%>
                            <a type="button" class="btn history-button btn-outline-dark"
                                href="/courses/<%= beta.course_array[x] %> "><%= beta.course_names[x] %> </a>
                            <% } %>
                            <% } %>
                            <% } %>
                        </div>
                    </div>
                </div>

                <div class="col-lg-10 main-right">
                    <div class="title-one"><Span>New Courses</Span></div>

                    <div class="latest-gallery">
                        <% let i=0 %>
                        <% for(let i=0;i<9;i++) {%>
                        <!-- <%= data[i].t_name %>  -->
                        <div class="gallery-item">
                            <div class="pop-up display-false" id="pop-<%= i %>">
                                <p> Done</p>
                            </div>
                            <div class="gallery-img">
                                <img src='/images/<%= i+1 %>.jpg' class="video-item"></div>
                            <div class="gallery-div">
                                <h4><%= data[i].c_name %> </h4>
                                <p style="margin:0">Teacher:<%= data[i].t_name %> <br> Domain: <%= data[i].domain %></p>
                                <div class="link-container">
                                    <div class="watch-div"><a href="/courses/<%= data[i].c_id %> "
                                            class="btn btn-secondary btn-sm btn-submit">Watch</a></div>
                                    <div class="btn-div">
                                        <form class="myForm" id="form-<%= i %>" method="POST"
                                            action="/playlist/<%= data[i].c_id %>">
                                            <button class="btn btn-secondary btn-sm  btn-submit" name="btn">
                                                +Playlist</button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% } %>
                    </div>
                    <div>
                        <hr style="border:none;height: 0;border-top:3px solid black ;">
                    </div>
                    <!--                    Here ends gallery section and starts latest section-->





                </div>
            </div>
    </main>


    <script src="/js/home.js"></script>


</body>

</html>